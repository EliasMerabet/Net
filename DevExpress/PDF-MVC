CONTROLLER.

using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using System.Web.Mvc;
using DemoDevExpressMVC.Models;
using DevExpress.XtraReports.UI;

namespace DemoDevExpressMVC.Controllers
{
    public class HomeController : Controller
    {
        private static int numStudents = 5;
        private static int numOrders = 500;
        public ActionResult Index()
        {
            ViewBag.Message = "Welcome to DevExpress Extensions for ASP.NET MVC!";
            XtraReport1 xr = new XtraReport1();
            List<Student> ls = getStudents();
            List<Order> lo = getOrders();

            /*foreach (Student s in ls)
            {
                XRTableRow tr = new XRTableRow();
                XRTableCell tc1 = new XRTableCell();
                XRTableCell tc2 = new XRTableCell();
                XRTableCell tc3 = new XRTableCell();
                XRTableCell tc4 = new XRTableCell();
                tc1.Text = s.NameS;
                tc2.Text = s.Surname;
                tc3.Text = s.DNI;
                tc4.Text = s.Address;
                tr.Cells.Add(tc1);
                tr.Cells.Add(tc2);
                tr.Cells.Add(tc3);
                tr.Cells.Add(tc4);
                xr.xrTable1.Rows.Add(tr);
            }*/

            xr.DataSource = lo;
            xr.xrLabel2.Text = "Current Date: " + DateTime.Now;
            xr.lbName.Text = ls[0].NameS;
            xr.lbSurname.Text = ls[0].Surname;
            xr.lbDNI.Text = ls[0].DNI;
            xr.lbAddress.Text = ls[0].Address;
            return View(xr);
        }

        private List<Student> getStudents()
        {
            List<Student> listStudent = new List<Student>();

            for (int i = 0; i < numStudents; i++)
            {
                Student s = new Student();
                s.NameS = "NameS " + i;
                s.Surname = "Surname " + i;
                s.Address = "Address " + i;
                s.DNI = "DNI " + i;

                listStudent.Add(s);
            }

            return listStudent;
        }

        private List<Order> getOrders()
        {
            List<Order> listOrder = new List<Order>();

            for (int i = 0; i < numOrders; i++)
            {
                Order o = new Order();
                o.OrderID = (i + 1000);
                o.Quantity = (i + 1000) - 900;
                o.Discount = 0.2F;
                o.Total = (o.Quantity * 10) - (o.Quantity * o.Discount);

                listOrder.Add(o);
            }

            return listOrder;
        }
    }
}

